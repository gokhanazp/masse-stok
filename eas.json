{
  "cli": {
    "version": ">= 16.27.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "image": "latest"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "ios": {
        "image": "latest"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "autoIncrement": true,
        "image": "latest"
      },
      "ios": {
        "autoIncrement": true,
        "image": "latest"
      }
    }
  }
}